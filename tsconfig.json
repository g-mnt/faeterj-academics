      "src/*": ["./src/*"],
      "assets/*": ["./src/assets/*"],
      "components/*": ["./src/components/*"],
      "hooks/*": ["./src/hooks/*"],
      "navigations/*": ["./src/navigations/*"],
      "repositories/*": ["./src/repositories/*"],
      "screens/*": ["./src/screens/*"],
